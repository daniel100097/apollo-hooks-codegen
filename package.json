{
  "name": "apollo-hooks-codegen",
  "version": "1.4.0",
  "description": "A plugin for graphql-code-generator to create fully typed React Hooks from GraphQL queries, mutations and subscriptions.",
  "keywords": [
    "react",
    "hooks",
    "graphql",
    "apollo",
    "codegen",
    "graphql-code-generator",
    "typescript"
  ],
  "files": [
    "/dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "try": "npm run build && npm run generate",
    "build": "tsc",
    "prepare": "npm run build",
    "generate": "gql-gen",
    "test": "jest",
    "graphql-demo:generate": "gql-gen -c codegen.test.yml"
  },
  "author": "pancomputationalist@mailbox.org",
  "license": "ISC",
  "peerDependencies": {
    "graphql-tag": "^2.10.1",
    "@graphql-codegen/cli": "^1.4.0",
    "graphql": "^14.4.2",
    "react": "^16.8.23",
    "@apollo/react-hooks": "^3.0.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.0",
    "@types/graphql": "^14.2.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link-http": "^1.5.15",
    "fs-extra": "^8.1.0",
    "graphql": "^14.4.2",
    "@graphql-codegen/cli": "^1.4.0",
    "jest": "^24.8.0",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "16.8.6",
    "@testing-library/react": "^8.0.7",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3",
    "unfetch": "^4.1.0"
  },
  "dependencies": {}
}
