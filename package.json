{
  "name": "apollo-hooks-codegen",
  "version": "1.4.1",
  "description": "A plugin for graphql-code-generator to create fully typed React Hooks from GraphQL queries, mutations and subscriptions.",
  "keywords": [
    "react",
    "hooks",
    "graphql",
    "apollo",
    "codegen",
    "graphql-code-generator",
    "typescript"
  ],
  "files": [
    "/dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "try": "npm run build && npm run generate",
    "build": "npx tsc",
    "prepare": "npm run build",
    "generate": "npx gql-gen",
    "test": "npx jest",
    "graphql-demo:generate": "npx gql-gen -c codegen.test.yml"
  },
  "author": "pancomputationalist@mailbox.org",
  "license": "ISC",
  "peerDependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@graphql-codegen/cli": "^1.4.0",
    "graphql": "^14.4.2",
    "graphql-tag": "^2.10.3",
    "react": "^16.8.23"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.5",
    "@testing-library/react": "^10.0.4",
    "@types/fs-extra": "^8.0.0",
    "@types/graphql": "^14.2.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^13.13.5",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "apollo-cache-inmemory": "^1.6.2",
    "apollo-client": "^2.6.3",
    "apollo-link-http": "^1.5.15",
    "fs-extra": "^8.1.0",
    "graphql": "^14.4.2",
    "jest": "^24.8.0",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-apollo": "^2.5.8",
    "react-dom": "16.13.1",
    "ts-jest": "^25.5.0",
    "typescript": "^3.8.3",
    "unfetch": "^4.1.0"
  },
  "dependencies": {}
}
